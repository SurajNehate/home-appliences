{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Netlify Dev (Angular + Functions)",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev:netlify"
      ],
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}